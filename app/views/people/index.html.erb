<h1>Address Book</h1>

<% @people.each do |person| %>
  <h4><%= person.name %></h4>
  <ul>
    <% person.addresses.each do |address| %>
      <li> <%= address.address_type %><br>
        <%= address.street_address_1 %> <%= address.city %>, <%= address.state %>
      </li>
    <% end %>
  </ul>
<% end %>
